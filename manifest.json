{
	"name": "Wayo",
	"manifest_version": 2,
	"version": "0.0.1.3",
	"icons": {
		"16": "img/icons/icon-16.png",
		"48": "img/icons/icon-48.png",
		"128": "img/icons/icon-128.png"
	},
    "browser_action": {
        "default_icon": {
            "16": "img/icons/icon-16.png",
            "48": "img/icons/icon-48.png",
            "128": "img/icons/icon-128.png"
        },
        "default_title": "Wayo Extension",
        "default_popup": "main.html"
    },
	"permissions": [
        "tabs",
        "storage",
        "background",
        "http://*/"
	]
}
